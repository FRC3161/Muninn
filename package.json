    "@testing-library/user-event": "^12.1.10",
    "bootstrap": "^4.6.0",
    "node-sass": "4.14.1",
    "react": "^17.0.1",
    "react-bootstrap": "^1.4.3",
    "react-dom": "^17.0.1",
